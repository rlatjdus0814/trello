       data: [],\n        }),\n        addListMode: true,\n        text: ''\n      });\n      console.log(this.state.currentId);\n    } \n    this.addList();\n  }\n\n  handleListEnter = (e) => {\n    if(e.key === 'Enter'){\n      this.addBtn();\n    }\n  }\n\n  deleteList = (id) => {\n    this.setState({\n      items: this.state.items.filter(carditem => carditem.id !== id)\n    });\n  }\n\n  listUpdate = (id, data) => {\n    this.setState({\n      items: this.state.items.map((carditem) => id === carditem.id ? {...carditem, ...data} : carditem)\n    });\n  }\n\n  render(){\n    const {items, styleT, styleF, text} = this.state;\n    return (\n      <div className=\"root\">\n        <Top />\n        <div className=\"top-hr\"></div>\n        <div className=\"container\">\n          <div className=\"wrap\">\n            <div className=\"content\">\n              {items.map((item) => <List key={item.id} id={item.id} item={item} title={item.title} data={item.data} onRemove={this.deleteList} onUpdate={this.listUpdate} />)}\n              <div className=\"listTrue\" onClick={this.addList} style={styleT}>\n                <div className=\"listClickBefore\">\n                  <div className=\"plus-btn\"><img src={plus} alt=\"plus\" /></div>\n                  <span>Add another list</span>\n                </div>\n              </div>\n              <div className=\"listFalse\" style={styleF}>\n                <div className=\"listClickAfter\">\n                  <div className=\"listTitle\"><input value={text} placeholder=\"Enter list title...\" onChange={this.handleListInput} onKeyPress={this.handleListEnter}></input></div>\n                  <div className=\"listAddCan\">\n                    <div className=\"addBtn\" onClick={this.addBtn}>Add List</div>\n                    <div className=\"canBtn\" onClick={this.addList}><img src={cancel} alt=\"cancel\" /></div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default App;\n","/Users/kimseoyeon/project/trello/src/components/Top.js",[],"/Users/kimseoyeon/project/trello/src/components/List/Todo.js",["49"],"import React, { Component } from 'react';\nimport '../../App.css';\nimport Textarea from '../Card/Textarea.js';\nimport cancel from '../../img/cancel.png';\nimport moredark from '../../img/menu-dark.png';\n\nclass Todo extends React.Component {\n  render() {\n    return (\n      <div className=\"todo\">\n        <div className=\"content-wrap\">\n          <div className=\"content-wrap-card\">\n            <div className=\"card-top\">\n              <div className=\"card-top-title\">To Do</div>\n              <div className=\"card-top-menu\"><img src={moredark} alt=\"menu\" /></div>\n            </div>\n            <div className=\"card-compose\">\n              <Textarea />\n              <div className=\"card-compose-create\">\n                <div className=\"create\">\n                  <div className=\"create-left\">\n                    <input className=\"create-btn\" type=\"submit\" value=\"Add Card\"></input>\n                    <div className=\"cancel-btn\" href=\"#\"><img src={cancel} alt=\"cancel\" /></div>\n                  </div>\n                  <div className=\"card-more\"><img src={moredark} alt=\"menu\" /></div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default Todo;",["50","51"],"/Users/kimseoyeon/project/trello/src/components/List/Doing.js",["52"],"import React, { Component } from 'react';\nimport '../../App.css';\nimport Cards from '../Card/Cards.js';\nimport Create from '../Card/Create.js';\nimport moredark from '../../img/menu-dark.png';\n\nclass Doing extends React.Component {\n  render() {\n    return (\n      <div className=\"doing\">\n        <div className=\"content-wrap\">\n          <div className=\"content-wrap-card\">\n            <div cl